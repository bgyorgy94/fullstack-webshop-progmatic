{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "plugins": ["prettier"],
    "extends": ["airbnb-base", "prettier"],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "prettier/prettier": ["error"],
        "no-console": "off",
        "func-names": "off"
    }
}
